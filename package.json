{
  "name": "osint-discord-bot",
  "version": "1.0.0",
  "description": "OSINT Discord bot for security reconnaissance - aggregates publicly available information",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy-commands": "tsx src/bot/deploy-commands.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4",
    "playwright": "^1.41.0",
    "winston": "^3.11.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
